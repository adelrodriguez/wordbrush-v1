    "bullmq": "5.1.9",
    "clsx": "^2.1.0",
    "crypto-js": "4.2.0",
    "date-fns": "3.3.1",
    "framer-motion": "^11.0.3",
    "http-status-codes": "2.3.0",
    "ioredis": "^5.3.2",
