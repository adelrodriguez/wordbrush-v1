{
  "name": "wordbrush",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:app:sentry": "./scripts/sentry-upload-sourcemaps.sh",
    "build:app": "remix vite:build",
    "build": "run-s build:app build:app:sentry database:deploy",
    "database:deploy": "prisma migrate deploy",
    "database:generate": "prisma generate",
    "database:migrate:create": "prisma migrate dev --name",
    "database:migrate:reset": "prisma migrate reset",
    "database:migrate": "prisma migrate dev",
    "database:prototype": "prisma db push",
    "database:seed": "prisma db seed",
    "dev:email": "email dev -d ./app/components/email",
    "dev:profile": "remix vite:dev --profile",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "run-p database:generate",
    "routes:generate": "routes-gen -d @routes-gen/remix",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
